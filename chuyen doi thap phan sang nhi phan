#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
         long long n;
   cin>>n;
   int x[100000];
   int run=0;
   while(n!=0)
   {
       x[run]=n%2;
       run++;
       n=n/2;
   }
   for(int i=run-1;i>=0;i--)
   {
       cout<<x[i];
   }
   cout<<endl;
    }
}
