string nhansolon(string a,string b)
{
    string c="";
    string tg="";
    int m=-1;
    for(int i=a.size()-1;i>=0;i--)
    {
        m++;
        tg=nhansonho(b,a[i]-'0');
// hàm nhân số nhỏ file trên
        for(int j=1;j<=m;j++)
            tg=tg+'0';
        c=cong(c,tg);
//hàm cộng 2 số lớn file trên
    }
    return c;
